<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="AppBanHang.App"
			 xmlns:m="using:AppBanHang.Models"
             xmlns:local="using:AppBanHang"
             RequestedThemeVariant="Light">
             <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->
  
    <Application.Styles>
        <FluentTheme />
    </Application.Styles>
	<Application.Resources>
		<ResourceDictionary>
			<ResourceDictionary.MergedDictionaries>
				<ResourceInclude Source="/Values/Texts.axaml"/>
				<ResourceInclude Source="/Values/Colors.axaml"/>
				<ResourceInclude Source="/Values/Numbers.axaml"/>
				<ResourceInclude Source="/Values/DataTemplates.axaml"/>
			</ResourceDictionary.MergedDictionaries>
		</ResourceDictionary>
	</Application.Resources>
	<Application.DataTemplates>
	</Application.DataTemplates>
</Application>